server.port=8082

##spring h2 database relates config
spring.datasource.url=jdbc:h2:mem:VCenterDB
spring.h2.console.enabled=true

# added below so that database is present before execution of data sql
spring.jpa.defer-datasource-initialization=true
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true
service.discovery.names.citizenService=CITIZEN-SERVICE